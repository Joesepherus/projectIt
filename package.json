{
  "name": "Math-web",
  "version": "1.0.0",
  "description": "React Web App about Maths",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "server": "nodemon ./server/server.js",
    "client": "node ./server/start-client.js",
    "build-css": "node-sass-chokidar ./client/src/ -o ./client/src/",
    "watch-css": "npm run build-css && node-sass-chokidar ./client/src/ -o ./client/src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "body-parser": "*",
    "bootstrap": "^4.0.0",
    "cors": "^2.8.4",
    "express": "*",
    "express-jwt": "^5.3.1",
    "jquery": "^3.3.1",
    "jwks-rsa": "^1.2.1",
    "mongoose": "*",
    "npm-lifecycle": "^2.0.1",
    "react-manager": "^1.0.4",
    "react-semantic-ui": "^0.2.0",
    "npm-run-all": "^4.1.3",
  },
  "author": "Jozef Maloch",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "create-react-class": "^15.6.3",
    "npm-run-all": "^4.1.3",
    "semantic-ui-react": "^0.81.1"
  }
}
